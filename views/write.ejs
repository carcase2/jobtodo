<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>COAD</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
  </head>
  <body>
    <%- include('nav.html') %> 
    <div class="container">
        <div class="form-group">
            <label>제목</label>
            <input type="text" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>요청일</label>
            <input type="date" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>마감예정일</label>
            <input type="date" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>요청자</label>
            <input type="text" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>세부내용</label>
            <input type="text" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>소요시간</label>
            <input type="number" class="form-control" name="title">
        </div>
        <div class="form-group">
            <label>완료일</label>
            <input type="date" class="form-control" name="title">
        </div>
        <label for="job_state">진행상태</label>

        <select id="job_state">
          <option value="going">진행중</option>
          <option value="done">완료</option>
          <option value="notstart" selected="selected">미진행</option>
          <option value="delay">보류</option>
        </select>

        <div class="form-group">
            <label>비고</label>
            <input type="text" class="form-control" name="title">
        </div>
         <label for="job_state">난이도</label>
         <select id="job_state">
          <option value="going">*</option>
          <option value="done">**</option>
          <option value="notstart">***</option>
          <option value="delay">****</option>
          <option value="delay">******</option>
        </select>
        



        
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
  </body>
</html>
